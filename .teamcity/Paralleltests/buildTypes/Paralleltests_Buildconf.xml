<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="20a310c5-9acc-43c1-ae3d-ea82efd53976" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2025.3/project-config.xsd">
  <name>buildconf</name>
  <description />
  <settings>
    <build-runners>
      <runner id="dotnet" name="" type="dotnet">
        <parameters>
          <param name="command" value="build" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="parallelexecution" />
    </vcs-settings>
    <build-extensions>
      <extension id="parallelTests" type="parallelTests">
        <parameters>
          <param name="numberOfBatches" value="2" />
        </parameters>
      </extension>
    </build-extensions>
  </settings>
</build-type>

